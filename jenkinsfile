            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
    }
}   
